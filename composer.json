{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "barryvdh/laravel-ide-helper": "^2.7",
        "beberlei/assert": "^3.2",
        "bensampo/laravel-enum": "^3.0",
        "calebporzio/sushi": "^2.1",
        "cloudradar/laravel-settings": "@dev",
        "doctrine/dbal": "2.12",
        "eduardokum/laravel-mail-auto-embed": "^1.0",
        "fideloper/proxy": "^4.2",
        "firebase/php-jwt": "^5.2",
        "fntneves/laravel-transactional-events": "^2.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "influxdb/influxdb-php": "^1.15",
        "intervention/image": "^2.5",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^8.10",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "pda/pheanstalk": "^4.0",
        "sentry/sentry-laravel": "^2.3",
        "spatie/data-transfer-object": "^2.5",
        "spatie/eloquent-sortable": "^3.9",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-json-api-paginate": "^1.9",
        "spatie/laravel-query-builder": "^3.3",
        "spatie/laravel-ray": "^1.3",
        "spatie/laravel-schemaless-attributes": "^1.8",
        "spatie/laravel-tags": "^3.0",
        "spatie/regex": "^1.4",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-query-detector": "^1.4",
        "brianium/paratest": "^5.0",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "itsgoingd/clockwork": "^4.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan view:clear",
            "@php artisan ide-helper:eloquent"
        ],
        "db:fresh": [
            "@php artisan migrate:fresh --seed"
        ],
        "db:fresh:test": [
            "@php artisan migrate:fresh --seed --env=testing"
        ],
        "mpt": [
            "php artisan migrate:fresh --seed --env=testing --quiet",
            "./vendor/bin/paratest --processes=half"
        ],
        "pt": [
          "./vendor/bin/paratest --processes=half"
        ],
        "mptw": [
            "php artisan migrate:fresh --seed --env=testing --quiet",
            "./vendor/bin/paratest --runner WrapperRunner --processes=half"
        ],
        "ptw": [
            "./vendor/bin/paratest --runner WrapperRunner --processes=half"
        ],
        "phpunit": [
            "php artisan migrate:fresh --seed --env=testing --quiet",
            "./vendor/bin/phpunit"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/laravel-settings",
            "options": {
                "symlink": false
            }
        }
    ]
}
